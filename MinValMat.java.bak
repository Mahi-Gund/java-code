class MinValMat 
{
	public static void main(String[] args) 
	{
		int i,j,min=0;
		int a[][]={{100,255,301},{46,50,69},{74,82,95}};
		min=a[0][0];
		for(i=0;i<3;i++)
		{
			for(j=0;j<3;j++)
			{
				System.out.print(a[i][j]+"\t");
				if (min>a[i][j])
				{
					min=a[i][j];
				}
			}
			System.out.println();
		}
		System.out.println("Min Value is"+min);
	}
}
