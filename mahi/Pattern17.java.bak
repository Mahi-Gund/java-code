class Pattern17 
{
	public static void main(String[] args) 
	{
		int i,j,k;
		int n=10;
		
		for(i=n;i>=1;i--)
		{
			for(k=1;k<=i;k++)
			{
				
				int sum=i+j;
				if(sum%2==0){
					System.out.print("1");
				}else{
					System.out.print("0");
				}
			}
			for(j=1;j<=n-1;j++)
			{ 
				System.out.print(" ");
			}
			
				System.out.println();
			}
			
		}
		
		
		
	}

